#: src/org.kde.kgoldrunner.desktop:2
#, 
msgctxt "Name"
msgid "KGoldrunner"
msgstr "KGoldrunner"

#: src/org.kde.kgoldrunner.desktop:72
msgctxt "GenericName"
msgid "Hunt Gold, Dodge Enemies and Solve Puzzles"
msgstr "Săn vàng, tránh kẻ thù và giải đố"

#: src/org.kde.kgoldrunner.desktop:124
msgctxt "Comment"
msgid "A game of action and puzzle-solving"
msgstr "Một trò chơi với hành động và giải đố"

#: themes/black-on-white.desktop:2
msgctxt "Name"
msgid "Black on White"
msgstr "Đen trên Trắng"

#: themes/black-on-white.desktop:59
msgctxt "Description"
msgid "A monochrome, black on white theme for KGoldrunner"
msgstr "Một chủ đề đơn sắc, đen trên nền trắng cho KGoldrunner"

#: themes/default.desktop:2
#, 
msgctxt "Name"
msgid "KGoldRunner Default"
msgstr "Mặc định của KGoldRunner"

#: themes/default.desktop:58
msgctxt "Description"
msgid "A light and clean theme for KDE4"
msgstr "Một chủ đề sáng và sạch sẽ cho KDE4"

#: themes/egypt.desktop:2
msgctxt "Name"
msgid "The Treasure of Egypt"
msgstr "Kho báu của Ai Cập"

#: themes/egypt.desktop:56
msgctxt "Description"
msgid "Help Matt Goldrunner escape the traps of ancient Egypt."
msgstr "Giúp Matt Goldrunner chuyển dịch khỏi các bẫy của Ai Cập cổ đại."

#: themes/kgr_geek.desktop:2
msgctxt "Name"
msgid "Geek City"
msgstr "Thành phố Geek"

#: themes/kgr_geek.desktop:55
msgctxt "Description"
msgid "The hero is trapped inside a computer."
msgstr "Người hùng bị mắc kẹt bên trong máy tính."

#: themes/nostalgia-blues.desktop:2
#, 
