msgid "There is a problem in the output format configuration."
msgstr "Có vấn đề trong cấu hình định dạng đầu ra."

#: data/scripts/media:302
msgid "media v%1 for Konversation."
msgstr "phương tiện v%1 cho Konversation."

#: data/scripts/media:303
msgid "Usage:"
msgstr "Cách sử dụng:"

#: data/scripts/media:304
msgid "\"/media\" - report what the first player found is playing."
msgstr "\"/media\" - báo cái gì mà trình phát đầu tiên đang phát."

#: data/scripts/media:305
msgid ""
"\"/media [ 'audio' | 'video' ]\" - report what is playing in a known audio "
"or video player."
msgstr ""
"\"/media [ 'audio' | 'video' ]\" - báo cái gì đang phát trong một trình "
"phát âm thanh hoặc phim đã biết."

#: data/scripts/media:306
msgid ""
"\"/media { Player }\" - report what is playing in the specified player if it "
"is found."
msgstr ""
"\"/media { Player }\" - báo cái gì đang phát trong trình phát được chỉ định "
"nếu nó được tìm thấy."

