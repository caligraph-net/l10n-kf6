#: maildispatcheragent.cpp:80 maildispatcheragent.cpp:224
#, kde-format
msgid "Sending messages (1 item in queue)…"
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "Đang gửi tin nhắn (%1 mục trong hàng đợi)..."

#: maildispatcheragent.cpp:90
#, kde-format
msgid "Sending canceled."
msgstr "Đã hủy gửi."

#: maildispatcheragent.cpp:97
#, kde-format
msgid "Finished sending messages."
msgstr "Đã hoàn thành việc gửi tin nhắn."

#: maildispatcheragent.cpp:103
#, kde-format
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "Đã gửi email thành công"

#: maildispatcheragent.cpp:104
#, kde-format
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "Email của bạn đã được gửi thành công."

#: maildispatcheragent.cpp:110
#, kde-format
msgid "No items in queue."
msgstr "Không có mục nào trong hàng đợi."

#: maildispatcheragent.cpp:155
#, kde-format
msgid "Online, sending messages in queue."
msgstr "Trực tuyến, đang gửi các tin nhắn trong hàng đợi."

#: maildispatcheragent.cpp:159
#, kde-format
msgid "Offline, message sending suspended."
msgstr "Ngoại tuyến, việc gửi thông điệp đã bị tạm dừng."

#: maildispatcheragent.cpp:184
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "Đang gửi: %1"

#: maildispatcheragent.cpp:249
#, kde-format
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "Gửi email thất bại"

#: maildispatcheragent.cpp:282
#, kde-format
msgid "Ready to dispatch messages."
msgstr "Sẵn sàng để gửi tin nhắn."

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "Mã định danh của hộp thư đi"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "Mã định danh của bộ sưu tập Thư đã gửi"

#: outboxqueue.cpp:265
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "Không thể truy cập vào thư mục hộp thư đi (%1)."

#: sendjob.cpp:49 sendjob.cpp:50
#, kde-format
msgid "Message sending aborted."
msgstr "Việc gửi thông điệp đã bị hủy bỏ."

#: sendjob.cpp:59
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr "Không thể bắt đầu vận chuyển thông điệp. Có thể là vận chuyển không hợp lệ."

#: sendjob.cpp:65
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "Không thể gửi tin nhắn. Giao thức vận chuyển không hợp lệ."

#: sendjob.cpp:97
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "Không thể lấy được giao diện D-Bus của tài nguyên %1."

#: sendjob.cpp:110
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "Hồi đáp D-Bus không hợp lệ từ tài nguyên %1."

#: sendjob.cpp:229
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr ""
"Thư mục thư đã gửi mặc định không khả dụng. Đang giữ thông điệp trong hộp "
"thư đi."

#: sendjob.cpp:241
#, kde-format
msgid "Message transport aborted."
msgstr "Truyền tải thông điệp đã bị hủy bỏ."

#: sendjob.cpp:241
#, kde-format
msgid "Failed to transport message."
msgstr "Không thể vận chuyển tin nhắn."

#: sendjob.cpp:283
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "Không thể lấy được giao diện D-Bus của mailfilteragent."

#: sendjob.cpp:290
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "Hồi đáp D-Bus không hợp lệ từ mailfilteragent"

#: sendjob.cpp:332
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr "Gửi thành công, nhưng không thể xoá bỏ thông điệp khỏi hộp thư đi."

#: sendjob.cpp:335
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr ""
"Gửi thành công, nhưng không thể di chuyển thông điệp vào thư mục thư đã "
"gửi."

#: sendjob.cpp:371
#, kde-format
msgid "Failed to store result in item."
msgstr "Không thể lưu kết quả vào mục."

#: storeresultjob.cpp:59
#, kde-format
msgid "Failed to fetch item."
msgstr "Không thể tìm nạp mục."