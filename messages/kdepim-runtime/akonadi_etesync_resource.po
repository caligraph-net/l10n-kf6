msgstr "Trình hướng dẫn cấu hình EteSync"

#: setupwizard.cpp:66
#, kde-format
msgid "Login Credentials"
msgstr "Thông tin đăng nhập"

#: setupwizard.cpp:67
#, kde-format
msgid "Enter your credentials to login to the EteSync server"
msgstr "Nhập thông tin đăng nhập của bạn để đăng nhập vào máy chủ EteSync"

#: setupwizard.cpp:76
