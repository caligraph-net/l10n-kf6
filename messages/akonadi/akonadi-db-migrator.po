#: dbmigrator.cpp:437
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "Đang dừng dịch vụ Akonadi..."

#: dbmigrator.cpp:440
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "Lỗi: hết giờ chờ Akonadi dừng."

#: dbmigrator.cpp:447
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "Lỗi: không thể có được khóa DBus cho Akonadi."

#: dbmigrator.cpp:456
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "Đang khởi động dịch vụ Akonadi..."

#: dbmigrator.cpp:469
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "Hoàn thành kiểm tra tệp cơ sở dữ liệu"

#: dbmigrator.cpp:483
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Động cơ cơ sở dữ liệu không hợp lệ \"%1\" - các giá trị hợp lệ là "
"\"sqlite\", \"mysql\" và \"postgres\"."

#: dbmigrator.cpp:505
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Lỗi: không thể cấu hình máy chủ cơ sở dữ liệu nguồn."

#: dbmigrator.cpp:510
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "Cơ sở dữ liệu nguồn và đích là giống nhau."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Lỗi: không thể lấy được đường dẫn đến tệp dữ liệu cơ sở dữ liệu nguồn hoặc "
"thư mục."

#: dbmigrator.cpp:524
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Lỗi: không thể cấu hình máy chủ cơ sở dữ liệu mới."

#: dbmigrator.cpp:530
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr "Lỗi: không thể mở cơ sở dữ liệu hiện có để di chuyển dữ liệu từ đó."

#: dbmigrator.cpp:535
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr "Lỗi: không thể mở cơ sở dữ liệu mới để di chuyển dữ liệu."

#: dbmigrator.cpp:540
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr "Đang chạy fsck trên cơ sở dữ liệu nguồn"

#: dbmigrator.cpp:559
#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr "Lỗi: không thể xoá thư mục cơ sở dữ liệu tạm thời %1: %2"

#: dbmigrator.cpp:566
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "Đang sao lưu cơ sở dữ liệu gốc..."

#: dbmigrator.cpp:574
#, kde-format
msgctxt "@info:status"
msgid "Restoring database from backup %1 to %2"
msgstr "Đang khôi phục cơ sở dữ liệu từ bản sao lưu %1 đến %2"

#: dbmigrator.cpp:576
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to restore database from backup: %1"
msgstr "Lỗi: không thể khôi phục cơ sở dữ liệu từ bản sao lưu: %1"

#: dbmigrator.cpp:608 dbmigrator.cpp:619
#, kde-format
msgctxt "@info:shell"
msgid "Error has occurred while migrating table %1"
msgstr "Đã xảy ra lỗi trong khi di chuyển bảng %1"

#: dbmigrator.cpp:638
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr "Thư mục sao lưu đã tồn tại. Bạn có muốn ghi đè lên sao lưu trước đó không?"

#: dbmigrator.cpp:643
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr ""
"Không thể tiến hành mà không có bản sao lưu. Quá trình di chuyển bị gián "
"đoạn."

#: dbmigrator.cpp:648
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Không thể xoá thư mục sao lưu trước đó."

#: dbmigrator.cpp:661
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "Không thể di chuyển cơ sở dữ liệu đến vị trí sao lưu: %1"

#: dbmigrator.cpp:675
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "Tệp sao lưu %1 đã tồn tại. Ghi đè?"

#: dbmigrator.cpp:677
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr ""
"Không thể tiến hành mà không có bản sao lưu. Quá trình di chuyển bị gián "
"đoạn."

#: dbmigrator.cpp:683
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "Lỗi: Không thể xoá tệp sao lưu hiện có %1: %2"

#: dbmigrator.cpp:690
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "Không thể sao lưu cấu hình Máy chủ Akonadi: %1"

#: dbmigrator.cpp:707
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr "Lỗi: không thể di chuyển cơ sở dữ liệu đã di chuyển đến vị trí chính: %1"

#: dbmigrator.cpp:726
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Lỗi: không thể xoá bỏ akonadiserverrc ban đầu: %1"

#: dbmigrator.cpp:732
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr "Lỗi: không thể di chuyển akonadiserverrc mới đến vị trí chính: %1"

#: dbmigrator.cpp:823
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "Lỗi: không thể cập nhật giá trị tự động tăng cho bảng %1"

#: dbmigrator.cpp:828
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Đang tối ưu hóa bảng %1..."

#: dbmigrator.cpp:831
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Lỗi: không thể tối ưu hóa bảng %1"

