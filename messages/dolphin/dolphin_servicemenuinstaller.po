#: servicemenuinstaller.cpp:442
#, kde-format
msgid "Command is required."
msgstr "Cần phải có lệnh."

#: servicemenuinstaller.cpp:445
#, kde-format
msgid "Path to archive is required."
msgstr "Cần phải có đường dẫn đến kho trữ."

