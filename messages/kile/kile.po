#: kilestdactions.cpp:51
#, kde-format
msgid "Quotation - \\begin{quotation}"
msgstr "Trích dẫn - \\begin{quotation}"

#: kilestdactions.cpp:51
#, kde-format
msgid "Quotation"
msgstr "Trích dẫn"

#: kilestdactions.cpp:51
#, kde-format
msgid ""
