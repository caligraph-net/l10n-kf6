#: kdeconnect-cli.cpp:43
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Làm cho --list-devices hoặc --list-available chỉ in ra ID của các thiết bị, "
"để dễ dàng viết script"

#: kdeconnect-cli.cpp:45
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Làm cho --list-devices hoặc --list-available chỉ in tên thiết bị, để dễ "
"dàng viết script"

#: kdeconnect-cli.cpp:47
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Làm cho --list-devices hoặc --list-available chỉ in ra mã định danh và tên "
"của các thiết bị, để dễ dàng viết script"

#: kdeconnect-cli.cpp:48
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Tìm kiếm các thiết bị trong mạng và thiết lập lại các kết nối"

#: kdeconnect-cli.cpp:49
#, kde-format
msgid "Request pairing to a said device"
msgstr "Yêu cầu ghép cặp với thiết bị đã chỉ định"

#: kdeconnect-cli.cpp:50
#, kde-format
msgid "Find the said device by ringing it."
msgstr "Tìm thiết bị được nói đến bằng cách rung nó."

#: kdeconnect-cli.cpp:51
#, kde-format
msgid "Stop pairing to a said device"
msgstr "Dừng ghép cặp với thiết bị đã chỉ định"

#: kdeconnect-cli.cpp:52
#, kde-format
msgid "Sends a ping to said device"
msgstr "Gửi một tín hiệu ping đến thiết bị đó"

#: kdeconnect-cli.cpp:53
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "Tương tự như ping nhưng bạn có thể đặt thông điệp để hiển thị"

#: kdeconnect-cli.cpp:53 kdeconnect-cli.cpp:60
#, kde-format
msgid "message"
msgstr "thông điệp"

#: kdeconnect-cli.cpp:54
#, kde-format
msgid "Sends the current clipboard to said device"
msgstr "Gửi bảng nháp hiện tại đến thiết bị đã chỉ định"

#: kdeconnect-cli.cpp:55
#, kde-format
msgid "Share a file/URL to a said device"
msgstr "Chia sẻ một tệp/URL với thiết bị đã chọn"

#: kdeconnect-cli.cpp:56
#, kde-format
msgid "Share text to a said device"
msgstr "Chia sẻ văn bản với thiết bị đã chọn"

#: kdeconnect-cli.cpp:57
#, kde-format
msgid "Display the notifications on a said device"
msgstr "Hiển thị các thông báo trên một thiết bị đã chỉ định"

#: kdeconnect-cli.cpp:58
#, kde-format
msgid "Lock the specified device"
msgstr "Khóa thiết bị được chỉ định"

#: kdeconnect-cli.cpp:59
#, kde-format
msgid "Unlock the specified device"
msgstr "Mở khóa thiết bị đã chỉ định"

#: kdeconnect-cli.cpp:60
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "Gửi tin nhắn SMS. Yêu cầu địa chỉ nhận"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "Phone number to send the message"
msgstr "Số điện thoại để gửi thông điệp"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "phone number"
msgstr "số điện thoại"

#: kdeconnect-cli.cpp:63
#, kde-format
msgid ""
"File urls to send attachments with the message (can be passed multiple times)"
msgstr "Các URL tệp để gửi kèm với thông điệp (có thể được truyền nhiều lần)"

#: kdeconnect-cli.cpp:64
#, kde-format
msgid "file urls"
msgstr "các URL tệp"

#: kdeconnect-cli.cpp:65
#, kde-format
msgid "Device ID"
msgstr "mã định danh"

#: kdeconnect-cli.cpp:66
#, kde-format
msgid "Device Name"
msgstr "Tên thiết bị"

#: kdeconnect-cli.cpp:67
#, kde-format
msgid "Get encryption info about said device"
msgstr "Lấy thông tin mật mã hoá về thiết bị đó"

#: kdeconnect-cli.cpp:68
#, kde-format
msgid "Lists remote commands and their ids"
msgstr "Liệt kê các lệnh ở xa và mã số của chúng"

#: kdeconnect-cli.cpp:69
#, kde-format
msgid "Executes a remote command by id"
msgstr "Thực thi một lệnh từ xa bằng mã định danh"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "Sends keys to a said device"
msgstr "Gửi các phím tới một thiết bị đã chỉ định"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Display this device's id and exit"
msgstr "Hiển thị mã định danh của thiết bị này và thoát"

