#: src/android_defaults.notifyrc:2
msgctxt "Name"
msgid "Fatal Error"
msgstr "Lỗi nghiêm trọng"

#: src/android_defaults.notifyrc:62
msgctxt "Comment"
msgid "There was a serious error causing the program to exit"
msgstr "Đã có một lỗi nghiêm trọng khiến chương trình phải thoát"

#: src/android_defaults.notifyrc:124
msgctxt "Name"
msgid "Notification"
msgstr "Thông báo"

#: src/android_defaults.notifyrc:183
msgctxt "Comment"
msgid "Something special happened in the program"
msgstr "Thứ gì đó đặc biệt đã xảy ra trong chương trình"

#: src/android_defaults.notifyrc:244
msgctxt "Name"
msgid "Warning"
msgstr "Cảnh báo"

#: src/android_defaults.notifyrc:303
msgctxt "Comment"
msgid "There was an error in the program which may cause problems"
msgstr "Có lỗi trong chương trình, có thể gây ra vấn đề"

#: src/android_defaults.notifyrc:364
msgctxt "Name"
msgid "Catastrophe"
msgstr "Thảm hoạ"

#: src/android_defaults.notifyrc:422
msgctxt "Comment"
msgid "A very serious error occurred, at least causing the program to exit"
msgstr ""
"Một lỗi rất nghiêm trọng đã xảy ra, ít nhất nó khiến chương trình phải thoát"
