#, kde-format
msgctxt "@title:window"
msgid "Sieve Manager"
msgstr "Trình quản lý Sieve"

#: kmanagesieve/sessionthread.cpp:299
#, kde-format
msgid ""
"Authentication failed.\n"
"Most likely the password is wrong.\n"
"The server responded:\n"
"%1"
msgstr ""
"Xác thực không thành công.\nGần như chắc chắn mật khẩu không đúng.\nMáy chủ "
"đã phản hồi:\n%1"

#: kmanagesieve/sievejob.cpp:142 kmanagesieve/sievejob.cpp:149
#, kde-format
msgid ""
"The script did not upload successfully.\n"
"This is probably due to errors in the script.\n"
"The server responded:\n"
"%1"
msgstr ""
"Kịch bản không được tải lên thành công.\nĐiều này có thể do lỗi trong kịch "
"bản.\nMáy chủ đã phản hồi:\n%1"

#: kmanagesieve/sievejob.cpp:154
#, kde-format
msgid ""
"The script did not upload successfully.\n"
"The script may contain errors."
msgstr "Kịch bản không được tải lên thành công.\nKịch bản có thể chứa lỗi."

#: ksieve/error.cpp:91
#, kde-format
msgid "Parse error: Carriage Return (CR) without Line Feed (LF)"
msgstr "Lỗi phân tách: Dấu xuống dòng (CR) không có Dấu mới dòng (LF)"

#: ksieve/error.cpp:95
#, kde-format
msgid ""
"Parse error: Unquoted Slash ('/') without Asterisk ('*'). Broken Comment?"
msgstr ""
"Lỗi phân tách: Dấu gạch chéo ('/') không có dấu sao ('*'). Bình luận bị "
"hỏng?"

#: ksieve/error.cpp:99
#, kde-format
msgid "Parse error: Illegal Character"
msgstr "Lỗi phân tách: Kí tự không hợp lệ"

#: ksieve/error.cpp:102
#, kde-format
msgid "Parse error: Unexpected Character, probably a missing space?"
msgstr "Lỗi phân tách: Ký tự bất ngờ, có lẽ là thiếu khoảng trắng?"

#: ksieve/error.cpp:105
#, kde-format
msgid "Parse error: Tag Name has leading Digits"
msgstr "Lỗi phân tách: Tên thẻ có chữ số đứng đầu"

#: ksieve/error.cpp:109
#, kde-format
msgid ""
"Parse error: Only whitespace and #comments may follow \"text:\" on the same "
"line"
msgstr ""
"Lỗi phân tách: Chỉ có khoảng trắng và #comments mới có thể theo sau "
"\"text:\" trên cùng một dòng"

#: ksieve/error.cpp:113
#, kde-format
msgid "Parse error: Number out of Range (must be smaller than %1)"
msgstr "Lỗi phân tách: Số nằm ngoài phạm vi (phải nhỏ hơn %1)"

#: ksieve/error.cpp:116
#, kde-format
msgid "Parse error: Invalid UTF-8 sequence"
msgstr "Lỗi phân tách: Chuỗi UTF-8 không hợp lệ"

#: ksieve/error.cpp:119
#, kde-format
msgid ""
"Parse error: Premature end of Multiline String (did you forget the '.'?)"
msgstr "Lỗi phân tách: Kết thúc chuỗi đa dòng quá sớm (bạn có quên dấu '.' không?)"

#: ksieve/error.cpp:122
#, kde-format
msgid "Parse error: Premature end of Quoted String (missing closing '\"')"
msgstr "Lỗi phân tách: Kết thúc chuỗi được trích dẫn quá sớm (thiếu dấu đóng '\"')"

#: ksieve/error.cpp:125
#, kde-format
msgid "Parse error: Premature end of String List (missing closing ']')"
msgstr "Lỗi phân tách: Kết thúc chuỗi danh sách quá sớm (thiếu dấu đóng ']')"

#: ksieve/error.cpp:128
#, kde-format
msgid "Parse error: Premature end of Test List (missing closing ')')"
msgstr "Lỗi phân tách: Kết thúc sớm của Danh sách Kiểm thử (thiếu dấu ')' đóng)"

#: ksieve/error.cpp:131
#, kde-format
msgid "Parse error: Premature end of Block (missing closing '}')"
msgstr "Lỗi phân tách: Kết thúc khối quá sớm (thiếu dấu ngoặc đóng '}')"

#: ksieve/error.cpp:134
#, kde-format
msgid "Parse error: Missing Whitespace"
msgstr "Lỗi phân tách: Thiếu khoảng trắng"

#: ksieve/error.cpp:137
#, kde-format
msgid "Parse error: Missing ';' or Block"
msgstr "Lỗi phân tách: Thiếu ';' hoặc Khối"

#: ksieve/error.cpp:140
#, kde-format
msgid "Parse error: Expected ';' or '{', got something else"
msgstr "Lỗi phân tách: Mong đợi ';' hoặc '{', nhưng lại nhận được cái khác"

#: ksieve/error.cpp:143 ksieve/error.cpp:158
#, kde-format
msgid "Parse error: Expected Command, got something else"
msgstr "Lỗi phân tách: Mong đợi Lệnh, nhưng lại nhận được cái gì đó khác"

#: ksieve/error.cpp:146
#, kde-format
msgid "Parse error: Trailing, Leading or Duplicate Commas in String List"
msgstr "Lỗi phân tách: Dấu phẩy thừa, thiếu hoặc trùng lặp trong Danh sách chuỗi"

#: ksieve/error.cpp:149
#, kde-format
msgid "Parse error: Trailing, Leading or Duplicate Commas in Test List"
msgstr "Lỗi phân tách: Dấu phẩy thừa, thiếu hoặc trùng lặp trong Danh sách Kiểm thử"

#: ksieve/error.cpp:152
#, kde-format
msgid "Parse error: Missing ',' between Strings in String List"
msgstr "Lỗi phân tách: Thiếu ',' giữa các chuỗi trong danh sách chuỗi"

#: ksieve/error.cpp:155
#, kde-format
msgid "Parse error: Missing ',' between Tests in Test List"
msgstr "Lỗi phân tách: Thiếu ',' giữa các Kiểm thử trong Danh sách Kiểm thử"

#: ksieve/error.cpp:161
#, kde-format
msgid "Parse error: Only Strings allowed in String Lists"
msgstr "Lỗi phân tách: Chỉ cho phép Chuỗi trong Danh sách Chuỗi"

#: ksieve/error.cpp:164
#, kde-format
msgid "Parse error: Only Tests allowed in Test Lists"
msgstr "Lỗi phân tách: Chỉ được phép có Kiểm thử trong Danh sách Kiểm thử"

#: ksieve/error.cpp:169
#, kde-format
msgid "\"require\" must be first command"
msgstr "\"require\" phải là lệnh đầu tiên"

#: ksieve/error.cpp:172
#, kde-format
msgid "\"require\" missing for command \"%1\""
msgstr "Thiếu \"require\" cho lệnh \"%1\""

#: ksieve/error.cpp:175
#, kde-format
msgid "\"require\" missing for test \"%1\""
msgstr "Thiếu \"require\" cho kiểm thử \"%1\""

#: ksieve/error.cpp:178
#, kde-format
msgid "\"require\" missing for comparator \"%1\""
msgstr "\"require\" bị thiếu cho bộ so sánh \"%1\""

#: ksieve/error.cpp:181
#, kde-format
msgid "Command \"%1\" not supported"
msgstr "Lệnh \"%1\" không được hỗ trợ"

#: ksieve/error.cpp:184
#, kde-format
msgid "Test \"%1\" not supported"
msgstr "Kiểm thử \"%1\" không được hỗ trợ"

#: ksieve/error.cpp:187
#, kde-format
msgid "Comparator \"%1\" not supported"
msgstr "Bộ so sánh \"%1\" không được hỗ trợ"

#: ksieve/error.cpp:190
#, kde-format
msgid "Site Policy Limit Violation: Test nesting too deep (max. %1)"
