#, kde-format
msgid "Authorized at:"
msgstr "Được ủy quyền tại:"

#: ui/DeviceView.qml:80
#, kde-format
msgid "Connected at:"
msgstr "Đã kết nối tại:"

#: ui/DeviceView.qml:85
#, kde-format
msgid "Enrolled at:"
msgstr "Đã đăng ký tại:"

#: ui/DeviceView.qml:89
#, kde-format
msgid "Yes"
msgstr "Có"

#: ui/DeviceView.qml:89
#, kde-format
