#, kde-format
msgid "Enable Thunderbolt devices"
msgstr "Bật các thiết bị Thunderbolt"

#: ui/DeviceList.qml:60
#, kde-format
msgid "No Thunderbolt devices connected"
msgstr "Không có thiết bị Thunderbolt được kết nối"

#: ui/DeviceList.qml:61
#, kde-format
msgid "Plug in a Thunderbolt device"
msgstr "Cắm thiết bị Thunderbolt"

